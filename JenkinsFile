pipeline{
    agent any
    tools {
        maven "MAVEN3"
        jdk "OracleJDK11"
    }

    stages{
        stage('Fetch Code'){
            steps{
                git branch: 'master', url: 'https://github.com/BzCod3r/partzforlessSring.git'
            }
        }

        stage('Build'){
            steps{
                sh 'mvn install -DskipTests'
            }

            post{
                success{
                    echo 'Archiving artifacts now..'
                }
            }
        }
    }

}